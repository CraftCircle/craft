    "express-openid-connect": "^2.18.0",
    "graphql": "^16.10.0",
    "graphql-tools": "^9.0.18",
    "graphql-upload": "17.0.0",
    "graphql-upload-minimal": "^1.6.1",
    "mocha": "^11.1.0",
    "moment": "^2.30.1",
    "multer": "^1.4.5-lts.2",
    "next-auth": "^4.24.11",
