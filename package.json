    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "2.1.0",
    "@nestjs/microservices": "^11.0.12",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.12",
    "@prisma/client": "^6.5.0",
